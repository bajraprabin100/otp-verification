{
  "name": "customer-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node src/swEnvBuild.js && vite",
    "build": "set NODE_ENV=production && tsc -p tsconfig.prod.json && node src/swEnvBuild.js && vite build",
    "preview": "vite preview --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "postinstall": "patch-package",
    "jest": "jest",
    "test": "set NODE_ENV=test&& jest"
  },
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.15.6",
    "@mui/x-date-pickers": "^6.19.4",
    "@tanstack/react-location": "^3.7.4",
    "@tanstack/react-query": "^5.17.19",
    "@tanstack/react-query-devtools": "^5.24.1",
    "@tanstack/react-router": "^1.8.3",
    "@types/crypto-js": "^4.2.2",
    "@wojtekmaj/react-daterange-picker": "^5.5.0",
    "axios": "^1.6.7",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.3.1",
    "dayjs": "^1.11.10",
    "dompurify": "^3.1.5",
    "dotenv": "^16.4.5",
    "firebase": "^10.11.0",
    "graphql-ws": "^5.16.0",
    "install": "^0.13.0",
    "jest-environment-jsdom": "^29.7.0",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "mui-one-time-password-input": "^2.0.1",
    "patch-package": "^8.0.0",
    "react": "^18.2.0",
    "react-date-range": "^2.0.0-alpha.4",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-icons": "^4.4.0",
    "react-image-crop": "latest",
    "react-material-ui-carousel": "^3.4.2",
    "react-router-dom": "^6.25.0",
    "react-toastify": "^10.0.5",
    "typescript-eslint": "7.9.0",
    "uuid": "^9.0.1",
    "vite-plugin-svgr": "^4.2.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.8",
    "@hookform/resolvers": "^3.3.4",
    "@tanstack/router-vite-plugin": "^1.8.2",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.48",
    "@types/react-date-range": "^1.4.9",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-require": "^0.0.0",
    "sass": "^1.77.8",
    "ts-jest": "^29.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@tanstack/react-query|mui-one-time-password-input)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "isolatedModules": true
      }
    }
  },
  "overrides": {
    "rollup": "npm:@rollup/wasm-node"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
